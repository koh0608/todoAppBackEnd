{
  "name": "cravy-api",
  "private": true,
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "prebuild": "rimraf dist",
    "build": "NODE_OPTIONS=--max_old_space_size=8192 nest build",
    "typecheck": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "dev": "nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@apollo/gateway": "^0.48.1",
    "@nestjs-query/query-graphql": "^0.30.0",
    "@nestjs-query/query-sequelize": "^0.30.0",
    "@nestjs/apollo": "^10.0.6",
    "@nestjs/common": "^8.4.0",
    "@nestjs/config": "^1.2.0",
    "@nestjs/core": "^8.4.0",
    "@nestjs/event-emitter": "^1.1.0",
    "@nestjs/graphql": "^10.0.6",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/mapped-types": "^1.0.1",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.4.0",
    "@nestjs/platform-socket.io": "^8.4.0",
    "@nestjs/schedule": "^1.0.2",
    "@nestjs/sequelize": "^8.0.0",
    "@nestjs/swagger": "^5.2.0",
    "@nestjs/throttler": "^2.0.0",
    "@nestjs/typeorm": "^8.0.3",
    "@nestjs/websockets": "^8.4.0",
    "@sendgrid/mail": "^7.6.1",
    "@types/bcrypt": "^5.0.0",
    "@types/formidable": "^1.2.2",
    "@types/superagent": "4.1.10",
    "apollo-server-express": "^3.6.3",
    "aws-sdk": "^2.1087.0",
    "axios": "^0.26.1",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "crypto": "^1.0.1",
    "dotenv": "^10.0.0",
    "fast-csv": "^4.3.6",
    "firebase-admin": "^10.0.2",
    "formidable": "^2.0.1",
    "graphql": "^16.3.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-relay": "^0.6.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-tools": "^7.0.5",
    "graphql-upload": "^11.0.0",
    "helmet": "^4.6.0",
    "joi": "^17.6.0",
    "libphonenumber-js": "^1.9.49",
    "lodash": "^4.17.21",
    "moment-timezone": "^0.5.33",
    "mysql2": "^2.3.3",
    "nanoid": "^3.3.1",
    "numeral": "^2.0.6",
    "passport": "^0.5.2",
    "passport-custom": "^1.1.1",
    "passport-facebook": "^3.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "ramda": "^0.27.2",
    "rand-token": "^1.0.1",
    "referral-codes": "^2.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7",
    "sequelize": "^6.17.0",
    "sequelize-typescript": "^2.1.3",
    "slugify": "^1.6.5",
    "stream-mime-type": "^1.0.2",
    "swagger-ui-express": "^4.3.0",
    "tmp": "^0.2.1",
    "ts-morph": "^14.0.0",
    "typeorm": "^0.2.45",
    "universal-cookie": "^4.0.4",
    "validator": "^13.6.0",
    "winston": "^3.6.0",
    "winston-daily-rotate-file": "^4.6.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.1",
    "@nestjs/testing": "^7.6.15",
    "@types/async": "^3.2.12",
    "@types/cookie-parser": "^1.4.2",
    "@types/cron": "^1.7.3",
    "@types/express": "^4.17.11",
    "@types/fluent-ffmpeg": "^2.1.17",
    "@types/graphql-iso-date": "^3.4.0",
    "@types/graphql-upload": "^8.0.11",
    "@types/jest": "^26.0.22",
    "@types/lodash": "^4.14.179",
    "@types/multer": "^1.4.7",
    "@types/node": "^14.18.12",
    "@types/passport": "^1.0.7",
    "@types/passport-facebook": "^2.1.10",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/ramda": "^0.27.66",
    "@types/sequelize": "^4.28.11",
    "@types/socket.io": "^3.0.2",
    "@types/supertest": "^2.0.10",
    "@types/tmp": "^0.2.1",
    "@types/uuid": "^8.3.4",
    "@types/validator": "^13.1.3",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^11.0.0",
    "prettier": "^2.2.1",
    "run-script-webpack-plugin": "^0.0.11",
    "sequelize-cli": "^6.4.1",
    "supertest": "^6.2.2",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.18",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.13.0",
    "typescript": "^4.6.3",
    "webpack": "^5.70.0",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleNameMapper": {
      "^@common/(.*)$": "<rootDir>/../src/common/$1",
      "^@configs/(.*)$": "<rootDir>/../src/configs/$1",
      "^@constants": "<rootDir>/../src/constants",
      "^@constants/(.*)$": "<rootDir>/../src/constants/$1",
      "^@decorators/(.*)$": "<rootDir>/../src/decorators/$1",
      "^@dto/(.*)$": "<rootDir>/../src/dto/$1",
      "^@entities": "<rootDir>/../src/entities",
      "^@entities/(.*)$": "<rootDir>/../src/entities/$1",
      "^@guards/(.*)$": "<rootDir>/../src/guards/$1",
      "^@hooks/(.*)$": "<rootDir>/../src/hooks/$1",
      "^@modules/(.*)$": "<rootDir>/../src/modules/$1",
      "^@services/(.*)$": "<rootDir>/../src/services/$1",
      "^@utils": "<rootDir>/../src/utils",
      "^@utils/(.*)$": "<rootDir>/../src/utils/$1"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint src"
    ],
    "{*.json,.{babelrc,eslintrc,prettierrc}}": [
      "prettier --ignore-path .eslintignore --parser json --write"
    ],
    "*.{css,scss}": [
      "prettier --ignore-path .eslintignore --single-quote --write"
    ],
    "*.{html,md,yml}": [
      "prettier --ignore-path .eslintignore --single-quote --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
